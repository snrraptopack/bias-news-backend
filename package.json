{
  "name": "bias-news-api",
  "version": "1.0.0",
  "description": "A sophisticated news analysis platform that analyzes bias using AI",
  "main": "dist/main.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,js --exec ts-node src/main.ts",
    "build": "tsc",
    "build:clean": "rimraf dist && tsc",
    "postinstall": "npm run build",
    "start": "node dist/main.js",
    "start:prod": "npm run build && node dist/main.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "news",
    "bias",
    "analysis",
    "ai",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.13.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "sqlite3": "^5.1.7"
  },
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.15",
    "@types/node": "^20.0.0",
    "@types/sqlite3": "^3.1.11",
    "nodemon": "^3.1.4",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  }
}
